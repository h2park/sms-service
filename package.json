{
  "name": "sms-service",
  "version": "1.3.0",
  "description": "Rest Service for Octoblu SMS messages",
  "main": "server.js",
  "scripts": {
    "start": "node command.js",
    "test": "mocha",
    "coverage": "nyc npm test",
    "mocha:json": "env NPM_ENV=test mocha --reporter json > coverage/mocha.json",
    "test:watch": "mocha -w -R mocha-multi --reporter-options spec=-,mocha-osx-reporter=-"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/octoblu/sms-service.git"
  },
  "keywords": [
    "sms",
    "IOT",
    "octoblu"
  ],
  "author": "Octoblu Inc",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/octoblu/sms-service/issues"
  },
  "homepage": "https://github.com/octoblu/sms-service",
  "dependencies": {
    "body-parser": "^1.9.0",
    "coffee-script": "^1.10.0",
    "colors": "^1.1.2",
    "debug": "^2.2.0",
    "errorhandler": "^1.2.0",
    "express": "^4.9.5",
    "express-meshblu-auth": "^6.0.0",
    "express-meshblu-healthcheck": "^1.0.1",
    "express-meshblu-ratelimit": "^1.0.3",
    "lodash": "^2.4.2",
    "meshblu-config": "^3.2.0",
    "morgan": "^1.3.2",
    "request": "^2.44.0"
  },
  "devDependencies": {
    "chai": "^3.5.0",
    "chai-subset": "^1.3.0",
    "coffee-script": "^1.10.0",
    "mocha": "^3.0.2",
    "mocha-multi": "^0.9.1",
    "mocha-osx-reporter": "^0.1.2",
    "nyc": "^8.3.0",
    "sinon": "^1.17.6",
    "sinon-chai": "^2.8.0"
  },
  "nyc": {
    "cache": true,
    "reporter": [
      "text-summary",
      "lcov",
      "json"
    ],
    "extension": [
      ".coffee"
    ]
  }
}
